<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="100.0" prefWidth="500.0" spacing="12.0" stylesheets="@editor.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <Label id="name-label" text="New Name" />
    <TextField id="name" />
    <Label text="Preview" />
    <TextField id="path" editable="false"/>
    <HBox spacing="10.0" alignment="CENTER_RIGHT">
      <children>
        <Button id="cancel" minWidth="80.0" mnemonicParsing="false" text="Cancel" defaultButton="false" />
	      <Button id="ok" minWidth="80.0" mnemonicParsing="false" text="OK" defaultButton="true" />
      </children>
    </HBox>
  </children>
  <padding>
    <Insets bottom="12.0" left="12.0" right="12.0" top="12.0" />
  </padding>
</VBox>
