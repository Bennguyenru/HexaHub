<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="50.0" prefWidth="500.0" spacing="12.0" stylesheets="@editor.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label id="message" text="A newer version of Defold has just been downloaded!"/>
      <Button id="ok" minWidth="80.0" mnemonicParsing="false" text="Update and Restart" AnchorPane.rightAnchor="0.0" AnchorPane.bottomAnchor="0.0" />
      <Button id="cancel" minWidth="80.0" mnemonicParsing="false" text="Skip Update" AnchorPane.rightAnchor="0.0" AnchorPane.bottomAnchor="0.0" />
   </children>
   <padding>
     <Insets bottom="12.0" left="12.0" right="12.0" top="12.0" />
   </padding>
</VBox>
