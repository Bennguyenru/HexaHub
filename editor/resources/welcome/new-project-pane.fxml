<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>

<VBox id="new-project-pane" HBox.hgrow="ALWAYS" xmlns="http://javafx.com/javafx">
    <HBox id="template-categories" styleClass="tab-header-pane" />
    <ListView id="template-list" styleClass="template-list-view" VBox.vgrow="ALWAYS" />
    <HBox styleClass="form-pane">
        <HBox HBox.hgrow="ALWAYS">
            <Label text="Location" />
            <HBox id="new-project-location-browse-field" HBox.hgrow="ALWAYS" />
        </HBox>
        <Button id="create-new-project-button" styleClass="df-button" text="Create New Project" />
    </HBox>
</VBox>
