   <extension
         point="org.eclipse.core.expressions.definitions">
      <definition
            id="com.dynamo.cr.guied.core.definitions.guiEditorActive">
         <with
               variable="activeEditorId">
            <equals
                  value="com.dynamo.cr.guied.GuiSceneEditor">
            </equals>
         </with>
      </definition>
      <definition
            id="com.dynamo.cr.guied.core.definitions.guiSceneSelected">
         <with
               variable="selection">
            <iterate
                  operator="or">
               <instanceof
                     value="com.dynamo.cr.guied.core.GuiSceneNode">
               </instanceof>
            </iterate>
         </with>
      </definition>
      <definition
            id="com.dynamo.cr.guied.core.definitions.nodesNodeSelected">
         <with
               variable="selection">
            <iterate
                  operator="or">
               <instanceof
                     value="com.dynamo.cr.guied.core.NodesNode">
               </instanceof>
            </iterate>
         </with>
      </definition>
      <definition
            id="com.dynamo.cr.guied.core.definitions.texturesNodeSelected">
         <with
               variable="selection">
            <iterate
                  operator="or">
               <instanceof
                     value="com.dynamo.cr.guied.core.TexturesNode">
               </instanceof>
            </iterate>
         </with>
      </definition>
      <definition
            id="com.dynamo.cr.guied.core.definitions.fontsNodeSelected">
         <with
               variable="selection">
            <iterate
                  operator="or">
               <instanceof
                     value="com.dynamo.cr.guied.core.FontsNode">
               </instanceof>
            </iterate>
         </with>
      </definition>
      <definition
            id="com.dynamo.cr.guied.core.definitions.layersNodeSelected">
         <with
               variable="selection">
            <iterate
                  operator="or">
               <instanceof
                     value="com.dynamo.cr.guied.core.LayersNode">
               </instanceof>
            </iterate>
         </with>
      </definition>
      <definition
            id="com.dynamo.cr.guied.core.definitions.layoutNodeSelected">
         <with
               variable="selection">
            <iterate
                  operator="or">
               <instanceof
                     value="com.dynamo.cr.guied.core.LayoutNode">
               </instanceof>
            </iterate>
         </with>
      </definition>
      <definition
            id="com.dynamo.cr.guied.core.definitions.layoutsNodeSelected">
         <with
               variable="selection">
            <iterate
                  operator="or">
               <instanceof
                     value="com.dynamo.cr.guied.core.LayoutsNode">
               </instanceof>
            </iterate>
         </with>
      </definition>
      <definition
            id="com.dynamo.cr.guied.core.definitions.guiNodeSelected">
         <with
               variable="selection">
            <iterate
                  operator="or">
               <instanceof
                     value="com.dynamo.cr.guied.core.GuiNode">
               </instanceof>
            </iterate>
         </with>
      </definition>
      <definition
            id="com.dynamo.cr.guied.core.definitions.anyNodeSelected">
         <with
               variable="selection">
            <count
                  value="+">
            </count>
         </with>
      </definition>
      <definition
            id="com.dynamo.cr.guied.core.definitions.templateChildNodeSelected">
         <with
               variable="selection">
            <test
                  property="com.dynamo.cr.guied.selection.isTemplateNodeChild">
            </test>
         </with>
      </definition>
      <definition
            id="com.dynamo.cr.guied.core.definitions.templateNodesSelected">
         <with
               variable="selection">
            <or>
               <iterate
                     operator="or">
                  <instanceof
                        value="com.dynamo.cr.guied.core.TemplateNode">
                  </instanceof>
               </iterate>
               <test
                     property="com.dynamo.cr.guied.selection.isTemplateNodeChild">
               </test>
            </or>
         </with>
      </definition>
   </extension>
