{
	"logs": {
		"editor-log": {
			"pattern": "editor2.*.log",
			"dir" : "."
		},
		"engine-log": {
			"pattern": "log.txt",
			"dir" : "."
		}
	},
	"steps": [
		["await-log" "editor-log" 25000 "project loaded"],
		["screen-capture" "project_loaded"],
		["press" "Shortcut+Shift+R"],
		["type" ".coll"],
		["press" "Down"],
		["press" "Enter"],
		["wait" 1000],
		["screen-capture" "collection"],
		["press" "Shortcut+Shift+R"],
		["type" ".script"],
		["press" "Down"],
		["press" "Enter"],
		["wait" 1000],
		["screen-capture" "script"],
		["press" "Shortcut+B"],
		["wait" 10000],
		["screen-capture" "build"],
		["switch-focus" 1],
		["type" "print('testing reload')"],
		["press" "Enter"],
		["press" "Shortcut+R"],
		["await-log" "engine-log" 2000 "testing reload"],
		["switch-focus" 1],
		["press" "Shortcut+Q"],
		["press" "Shortcut+Q"],
	],
}