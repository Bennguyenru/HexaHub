{
	"logs": {
		"editor-log": {
			"pattern": "editor2.*.log",
			"dir" : "."
		},
		"engine-log": {
			"pattern": "log.txt",
			"dir" : "."
		}
	},
	"steps": [
		["await-log" "editor-log" 25000 "project loaded"],
		["wait" 2000],
		["screen-capture" "project_loaded"],
		["press" "Shortcut+Shift+R"],
		["type" ".coll"],
		["press" "Down"],
		["press" "Enter"],
		["wait" 2000],
		["screen-capture" "collection"],
		["press" "Shortcut+Shift+R"],
		["type" "player.script"],
		["press" "Down"],
		["press" "Enter"],
		["wait" 2000],
		["screen-capture" "script"],
		["press" "Shortcut+B"],
		["wait" 2000],
		["screen-capture" "game_intro"],
		["wait" 8000],
		["screen-capture" "game_running"],
		["switch-focus" 1],
		["press" "Shortcut+R"],
		["await-log" "engine-log" 2000 "successfully reloaded"],
		["screen-capture" "test_end"],
		["switch-focus" 1],
		["press" "Shortcut+Q"],
		["press" "Shortcut+Q"],
	],
}